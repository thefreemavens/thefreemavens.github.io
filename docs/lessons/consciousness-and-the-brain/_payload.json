ry",{"title":144,"_path":145},"Closing Remarks","/lessons/recommended-books",[147],{"title":148,"_path":149,"children":150},"Challenges","/challenges",[151],{"title":148,"_path":152},"/challenges/main",[154],{"title":155,"_path":156,"children":157},"Resources","/resources",[158,160,163],{"title":155,"_path":159},"/resources/main",{"title":161,"_path":162},"Definitions","/resources/definitions",{"title":164,"_path":165},"Awesome Natural Law","/resources/awesome-natural-law",{"_path":46,"_dir":167,"_draft":168,"_partial":168,"_locale":169,"title":45,"description":169,"cat":170,"id":171,"body":172,"_type":286,"_id":287,"_source":288,"_file":289,"_extension":290},"lessons",false,"","Lesson","12",{"type":173,"children":174,"toc":278},"root",[175,184,200,209,216,222,227,233,238,244,249,255,260,265,270],{"type":176,"tag":177,"props":178,"children":180},"element","h2",{"id":179},"definition",[181],{"type":182,"value":183},"text","Definition",{"type":176,"tag":185,"props":186,"children":187},"p",{},[188,198],{"type":176,"tag":189,"props":190,"children":191},"strong",{},[192],{"type":176,"tag":193,"props":194,"children":195},"em",{},[196],{"type":182,"value":197},"Consciousness",{"type":182,"value":199}," is the ability of a being to recognise patterns and meaning with respect to events taking place; both within oneself, and in the realm in which the being exists and operates. In other words...",{"type":176,"tag":201,"props":202,"children":203},"mono",{},[204],{"type":176,"tag":185,"props":205,"children":206},{},[207],{"type":182,"value":208},"Consciousness is the ability to accurately perceive truth; that which has happened, and that which is happening at the present moment.",{"type":176,"tag":210,"props":211,"children":215},"hr",{"className":212,"id":169},[213,214],"my-8","border-b-4",[],{"type":176,"tag":177,"props":217,"children":219},{"id":218},"expressions-of-consciousness",[220],{"type":182,"value":221},"Expressions of Consciousness",{"type":176,"tag":185,"props":223,"children":224},{},[225],{"type":182,"value":226},"The abilities we have, to make ourselves known to any other being around us. These expressions are:",{"type":176,"tag":177,"props":228,"children":230},{"id":229},"thoughts",[231],{"type":182,"value":232},"Thoughts",{"type":176,"tag":185,"props":234,"children":235},{},[236],{"type":182,"value":237},"Creative (neutral) force. (both Masculine and Feminine).",{"type":176,"tag":177,"props":239,"children":241},{"id":240},"emotions",[242],{"type":182,"value":243},"Emotions:",{"type":176,"tag":185,"props":245,"children":246},{},[247],{"type":182,"value":248},"Internal expression. (Feminine).",{"type":176,"tag":177,"props":250,"children":252},{"id":251},"actions",[253],{"type":182,"value":254},"Actions",{"type":176,"tag":185,"props":256,"children":257},{},[258],{"type":182,"value":259},"Result of thought and emotion being expressed into the physical realm. (Masculine)",{"type":176,"tag":185,"props":261,"children":262},{},[263],{"type":182,"value":264},"Like in almost all major religious traditions, this represents the ‘Holy Trinity’:\nThe father creator (thoughts), and the sacred feminine figure (emotions), which the father\nfigure impregnates or inseminates in some way. From that offspring a male child is born, always. The birth of this male child symbolises that the ‘saviour’ of humanity will be action; faith alone will not be enough.",{"type":176,"tag":185,"props":266,"children":267},{},[268],{"type":182,"value":269},"Even in modern Christianity, in their own exoteric scriptures, there is only one prescription\nthat the Christ-figure gives to the people when asked about freedom (John 8:32) -",{"type":176,"tag":201,"props":271,"children":272},{},[273],{"type":176,"tag":185,"props":274,"children":275},{},[276],{"type":182,"value":277},"You shall know the truth, and the truth shall set you free.",{"title":169,"searchDepth":279,"depth":279,"links":280},2,[281,282,283,284,285],{"id":179,"depth":279,"text":183},{"id":218,"depth":279,"text":221},{"id":229,"depth":279,"text":232},{"id":240,"depth":279,"text":243},{"id":251,"depth":279,"text":254},"markdown","content:1.lessons:12.consciousness-and-the-brain.md","content","1.lessons/12.consciousness-and-the-brain.md","md"]