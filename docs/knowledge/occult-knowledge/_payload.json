":143},"/lessons/main",[145],{"title":146,"_path":147,"children":148},"Challenges","/challenges",[149],{"title":146,"_path":150},"/challenges/main",[152],{"title":153,"_path":154,"children":155},"Resources","/resources",[156,158,161],{"title":153,"_path":157},"/resources/main",{"title":159,"_path":160},"Definitions","/resources/definitions",{"title":162,"_path":163},"Awesome Natural Law","/resources/awesome-natural-law",{"_path":22,"_dir":165,"_draft":166,"_partial":166,"_locale":167,"title":21,"description":167,"cat":168,"id":169,"body":170,"_type":310,"_id":311,"_source":312,"_file":313,"_extension":314},"knowledge",false,"","Lesson","04",{"type":171,"children":172,"toc":304},"root",[173,182,196,243,252,277,282,288,293,299],{"type":174,"tag":175,"props":176,"children":178},"element","h2",{"id":177},"hidden-knowledge",[179],{"type":180,"value":181},"text","Hidden Knowledge",{"type":174,"tag":183,"props":184,"children":185},"p",{},[186,188,194],{"type":180,"value":187},"This knowledge is \"Occulted\", meaning it ",{"type":174,"tag":189,"props":190,"children":191},"s",{},[192],{"type":180,"value":193},"is",{"type":180,"value":195}," was \"Hidden\".",{"type":174,"tag":183,"props":197,"children":198},{},[199,201,211,213,221,223,231,233,241],{"type":180,"value":200},"Derived from the Latin adjective ",{"type":174,"tag":202,"props":203,"children":204},"em",{},[205],{"type":174,"tag":206,"props":207,"children":208},"strong",{},[209],{"type":180,"value":210},"Occultus",{"type":180,"value":212}," which means \"concealed, hidden from sight\", which is in turn derived from the Latin verb ",{"type":174,"tag":202,"props":214,"children":215},{},[216],{"type":174,"tag":206,"props":217,"children":218},{},[219],{"type":180,"value":220},"Occultare",{"type":180,"value":222}," which means \"to keep secret, to conceal.\" Just like in English, the word ",{"type":174,"tag":202,"props":224,"children":225},{},[226],{"type":174,"tag":206,"props":227,"children":228},{},[229],{"type":180,"value":230},"Ocular",{"type":180,"value":232}," means \"related to the eye, related to sight\", which is derived from the Latin noun ",{"type":174,"tag":202,"props":234,"children":235},{},[236],{"type":174,"tag":206,"props":237,"children":238},{},[239],{"type":180,"value":240},"Oculus",{"type":180,"value":242}," meaning \"Eye\".",{"type":174,"tag":244,"props":245,"children":246},"mono",{},[247],{"type":174,"tag":183,"props":248,"children":249},{},[250],{"type":180,"value":251},"Occult Knowledge is certain information that has been hidden from sight for a specific reason.",{"type":174,"tag":183,"props":253,"children":256},{"className":254},[255],"desc",[257,259,268,270,275],{"type":180,"value":258},"Occult Knowledge constitutes both the Knowledge of ",{"type":174,"tag":260,"props":261,"children":265},"span",{"className":262},[263,264],"font-bold","border-b",[266],{"type":180,"value":267},"Human Consciousness",{"type":180,"value":269}," and how it operates, and the Knowledge of ",{"type":174,"tag":260,"props":271,"children":273},{"className":272},[263,264],[274],{"type":180,"value":24},{"type":180,"value":276},", the unseen universal spiritual laws which govern the consequences of human behaviour.",{"type":174,"tag":183,"props":278,"children":279},{},[280],{"type":180,"value":281},"In the ancient mystery traditions and occult schools, these concepts are known as the two ‘Arcana’ which translates to ‘Secret’ in both Latin and Greek (‘Arcanum (μυστικό)’ in Greek)",{"type":174,"tag":175,"props":283,"children":285},{"id":284},"minor-arcana",[286],{"type":180,"value":287},"Minor Arcana",{"type":174,"tag":183,"props":289,"children":290},{},[291],{"type":180,"value":292},"The Lesser (Minor) Arcana: Hidden knowledge of the microcosm. – Deals with individual units of consciousness; the human psyche.",{"type":174,"tag":175,"props":294,"children":296},{"id":295},"major-arcana",[297],{"type":180,"value":298},"Major Arcana",{"type":174,"tag":183,"props":300,"children":301},{},[302],{"type":180,"value":303},"The Greater (Major) Arcana: Hidden knowledge of the macrocosm – Understanding of the\nlarge (universal) laws of Nature that govern the macrocosm. → Natural Law,",{"title":167,"searchDepth":305,"depth":305,"links":306},2,[307,308,309],{"id":177,"depth":305,"text":181},{"id":284,"depth":305,"text":287},{"id":295,"depth":305,"text":298},"markdown","content:1.knowledge:04.occult-knowledge.md","content","1.knowledge/04.occult-knowledge.md","md"]