":206,"value":229},"Rightful possession (acquired without doing somebody else harm)",{"type":189,"tag":225,"props":231,"children":232},{},[233],{"type":206,"value":234},"Control of usage",{"type":189,"tag":225,"props":236,"children":237},{},[238],{"type":206,"value":239},"Maintaining personal responsibility.",{"type":189,"tag":241,"props":242,"children":243},"mono",{},[244],{"type":189,"tag":196,"props":245,"children":246},{},[247],{"type":206,"value":248},"Natural Law can essentially be reduced to a single spiritual law: Do Not steal.",{"type":189,"tag":209,"props":250,"children":252},{"id":251},"all-rights-are-property-rights",[253],{"type":206,"value":254},"All Rights Are Property Rights",{"type":189,"tag":196,"props":256,"children":257},{},[258],{"type":206,"value":259},"We have to collectively understand that all rights are property rights. Stop taking the property of other beings, and stop condoning the taking of property of other beings.",{"title":182,"searchDepth":261,"depth":261,"links":262},2,[263,264],{"id":211,"depth":261,"text":214},{"id":251,"depth":261,"text":254},"markdown","content:1.knowledge:26.ownership.md","content","1.knowledge/26.ownership.md","md",1697962979391]