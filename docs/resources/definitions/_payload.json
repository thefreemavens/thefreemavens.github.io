children":228},{},[229],{"type":180,"value":230},"The opposite of the conditions which we truly want to manifest into physical reality.",{"type":174,"tag":175,"props":232,"children":234},{"id":233},"consciousness",[235],{"type":180,"value":236},"Consciousness",{"type":174,"tag":183,"props":238,"children":239},{},[240],{"type":180,"value":241},"The ability of a being to recognise patterns and meaning within oneself, and in the realm in which it exists.",{"type":174,"tag":175,"props":243,"children":245},{"id":244},"definition",[246],{"type":180,"value":247},"Definition",{"type":174,"tag":183,"props":249,"children":250},{},[251],{"type":180,"value":252},"Exact statement or description of the nature, scope, or meaning of a word, concept, or object.",{"type":174,"tag":175,"props":254,"children":256},{"id":255},"diagnosis",[257],{"type":180,"value":258},"Diagnosis",{"type":174,"tag":183,"props":260,"children":261},{},[262],{"type":180,"value":263},"By way of knowledge. (Greek: Dia - Gnosis)",{"type":174,"tag":175,"props":265,"children":267},{"id":266},"force",[268],{"type":180,"value":269},"Force",{"type":174,"tag":183,"props":271,"children":272},{},[273],{"type":180,"value":274},"Action that causes physical change which is in harmony with Natural Law; because it doesn’t cause harm to other sentient beings.",{"type":174,"tag":175,"props":276,"children":278},{"id":277},"great-work",[279],{"type":180,"value":280},"Great Work",{"type":174,"tag":183,"props":282,"children":283},{},[284],{"type":180,"value":285},"The work of teaching Natural Law to others; after one has come to a definite understanding of its workings.",{"type":174,"tag":175,"props":287,"children":289},{"id":288},"ignorance",[290],{"type":180,"value":291},"Ignorance",{"type":174,"tag":183,"props":293,"children":294},{},[295],{"type":180,"value":296},"To not know something even though necessary information is present; because that information is wilfully being ignored. (Latin: Ignorare)",{"type":174,"tag":175,"props":298,"children":300},{"id":299},"initiation",[301],{"type":180,"value":302},"Initiation",{"type":174,"tag":183,"props":304,"children":305},{},[306],{"type":180,"value":307},"To Begin. To Introduce. To Start.",{"type":174,"tag":175,"props":309,"children":311},{"id":310},"jurisdiction",[312],{"type":180,"value":313},"Jurisdiction",{"type":174,"tag":183,"props":315,"children":316},{},[317],{"type":180,"value":318},"To say what the law is; to “decide” what right and wrong are. (Latin: Jus - Dictere)",{"type":174,"tag":175,"props":320,"children":322},{"id":321},"justification",[323],{"type":180,"value":324},"Justification",{"type":174,"tag":183,"props":326,"children":327},{},[328],{"type":180,"value":329},"To “make” or “create” a right. (Latin: Jus - Facere)",{"type":174,"tag":175,"props":331,"children":333},{"id":332},"knowledge",[334],{"type":180,"value":5},{"type":174,"tag":183,"props":336,"children":337},{},[338],{"type":180,"value":339},"Available information.",{"type":174,"tag":175,"props":341,"children":343},{"id":342},"law",[344],{"type":180,"value":345},"Law",{"type":174,"tag":183,"props":347,"children":348},{},[349],{"type":180,"value":350},"An existing condition which is both binding and immutable.",{"type":174,"tag":175,"props":352,"children":354},{"id":353},"natural",[355],{"type":180,"value":356},"Natural",{"type":174,"tag":183,"props":358,"children":359},{},[360],{"type":180,"value":361},"Having come from Spirit / God. (Egyptian: Neter)",{"type":174,"tag":175,"props":363,"children":365},{"id":364},"natural-law",[366],{"type":180,"value":24},{"type":174,"tag":183,"props":368,"children":369},{},[370],{"type":180,"value":371},"Natural Law is a set of universal, objective, eternal, non-man-made, binding and immutable conditions which govern the consequences of behaviours of beings with the capacity for understanding the difference between harmful and non-harmful actions.",{"type":174,"tag":175,"props":373,"children":375},{"id":374},"nescience",[376],{"type":180,"value":377},"Nescience",{"type":174,"tag":183,"props":379,"children":380},{},[381],{"type":180,"value":382},"To not know something because necessary information was absent or unattainable. (Latin: Nescire)",{"type":174,"tag":175,"props":384,"children":386},{"id":385},"occult",[387],{"type":180,"value":388},"Occult",{"type":174,"tag":183,"props":390,"children":391},{},[392],{"type":180,"value":393},"Hidden from sight. (Latin: Occultus / Occultare)",{"type":174,"tag":175,"props":395,"children":397},{"id":396},"order",[398],{"type":180,"value":399},"Order",{"type":174,"tag":183,"props":401,"children":402},{},[403],{"type":180,"value":404},"The conditions which we truly want to manifest into physical reality.",{"type":174,"tag":175,"props":406,"children":408},{"id":407},"order-follower",[409],{"type":180,"value":410},"Order-Follower",{"type":174,"tag":183,"props":412,"children":413},{},[414],{"type":180,"value":415},"A person that causes the currently existing system of slavery to remain in place.",{"type":174,"tag":175,"props":417,"children":419},{"id":418},"petition",[420],{"type":180,"value":421},"Petition",{"type":174,"tag":183,"props":423,"children":424},{},[425],{"type":180,"value":426},"A formally drawn request, that is addressed to a person or group of persons in authority or power, soliciting some favour, right, mercy, or other benefit.",{"type":174,"tag":175,"props":428,"children":430},{"id":429},"principle",[431],{"type":180,"value":432},"Principle",{"type":174,"tag":183,"props":434,"children":435},{},[436],{"type":180,"value":437},"That which comes first; that which is most important. (Latin: Principia)",{"type":174,"tag":175,"props":439,"children":441},{"id":440},"religion",[442],{"type":180,"value":443},"Religion",{"type":174,"tag":183,"props":445,"children":446},{},[447],{"type":180,"value":448},"To hold back from forward progress. (Latin: Religare)",{"type":174,"tag":175,"props":450,"children":452},{"id":451},"responsibility",[453],{"type":180,"value":454},"Responsibility",{"type":174,"tag":183,"props":456,"children":457},{},[458],{"type":180,"value":459},"The ability to respond.",{"type":174,"tag":175,"props":461,"children":463},{"id":462},"right",[464],{"type":180,"value":465},"Right",{"type":174,"tag":183,"props":467,"children":468},{},[469],{"type":180,"value":470},"Correct and Moral. Any action which does not cause harm to another sentient being or their property.",{"type":174,"tag":175,"props":472,"children":474},{"id":473},"self-loathing",[475],{"type":180,"value":476},"Self-Loathing",{"type":174,"tag":183,"props":478,"children":479},{},[480],{"type":180,"value":481},"The underlying psychological condition that causes people to attempt to abdicate their own personal responsibility to exercise conscience, and fall into patterns of order-following and justification.",{"type":174,"tag":175,"props":483,"children":485},{"id":484},"solipsist",[486],{"type":180,"value":487},"Solipsist",{"type":174,"tag":183,"props":489,"children":490},{},[491],{"type":180,"value":492},"A person that believes they are the only being in existence (Latin: Solus - Ipse)",{"type":174,"tag":175,"props":494,"children":496},{"id":495},"statist",[497],{"type":180,"value":498},"Statist",{"type":174,"tag":183,"props":500,"children":501},{},[502],{"type":180,"value":503},"A person who erroneously believes in “authority.”",{"type":174,"tag":175,"props":505,"children":507},{"id":506},"truth",[508],{"type":180,"value":509},"Truth",{"type":174,"tag":183,"props":511,"children":512},{},[513],{"type":180,"value":514},"All that has happened in the past, and that which is happening in the present moment; that which has been, and that which is.",{"type":174,"tag":175,"props":516,"children":518},{"id":517},"violence",[519],{"type":180,"value":520},"Violence",{"type":174,"tag":183,"props":522,"children":523},{},[524],{"type":180,"value":525},"Action that causes physical change which is in not harmony with Natural Law; because it causes harm to other sentient beings.",{"type":174,"tag":175,"props":527,"children":529},{"id":528},"wisdom",[530],{"type":180,"value":531},"Wisdom",{"type":174,"tag":183,"props":533,"children":534},{},[535],{"type":180,"value":536},"Either action, or inaction; based on knowledge and understanding.",{"type":174,"tag":175,"props":538,"children":540},{"id":539},"wrong",[541],{"type":180,"value":542},"Wrong",{"type":174,"tag":183,"props":544,"children":545},{},[546],{"type":180,"value":547},"Incorrect / immoral; actions that cause harm to other sentient beings.",{"title":167,"searchDepth":549,"depth":549,"links":550},2,[551,552,553,554,555,556,557,558,559,560,561,562,563,564,565,566,567,568,569,570,571,572,573,574,575,576,577,578,579,580,581,582],{"id":177,"depth":549,"text":181},{"id":190,"depth":549,"text":193},{"id":222,"depth":549,"text":225},{"id":233,"depth":549,"text":236},{"id":244,"depth":549,"text":247},{"id":255,"depth":549,"text":258},{"id":266,"depth":549,"text":269},{"id":277,"depth":549,"text":280},{"id":288,"depth":549,"text":291},{"id":299,"depth":549,"text":302},{"id":310,"depth":549,"text":313},{"id":321,"depth":549,"text":324},{"id":332,"depth":549,"text":5},{"id":342,"depth":549,"text":345},{"id":353,"depth":549,"text":356},{"id":364,"depth":549,"text":24},{"id":374,"depth":549,"text":377},{"id":385,"depth":549,"text":388},{"id":396,"depth":549,"text":399},{"id":407,"depth":549,"text":410},{"id":418,"depth":549,"text":421},{"id":429,"depth":549,"text":432},{"id":440,"depth":549,"text":443},{"id":451,"depth":549,"text":454},{"id":462,"depth":549,"text":465},{"id":473,"depth":549,"text":476},{"id":484,"depth":549,"text":487},{"id":495,"depth":549,"text":498},{"id":506,"depth":549,"text":509},{"id":517,"depth":549,"text":520},{"id":528,"depth":549,"text":531},{"id":539,"depth":549,"text":542},"markdown","content:4.resources:2.definitions.md","content","4.resources/2.definitions.md","md"]