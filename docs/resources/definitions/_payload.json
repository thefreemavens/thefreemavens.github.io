ype":188,"tag":214,"props":225,"children":226},{},[227],{"type":188,"tag":218,"props":228,"children":229},{},[230],{"type":194,"value":231},"Arcanum",{"type":194,"value":233},")",{"type":188,"tag":189,"props":235,"children":237},{"id":236},"chaos",[238],{"type":194,"value":239},"Chaos",{"type":188,"tag":197,"props":241,"children":242},{},[243],{"type":194,"value":244},"The opposite of the conditions which we truly want to manifest into physical reality.",{"type":188,"tag":189,"props":246,"children":248},{"id":247},"consciousness",[249],{"type":194,"value":250},"Consciousness",{"type":188,"tag":197,"props":252,"children":253},{},[254],{"type":194,"value":255},"The ability of a being to recognise patterns and meaning within oneself, and in the realm in which it exists.",{"type":188,"tag":189,"props":257,"children":259},{"id":258},"definition",[260],{"type":194,"value":261},"Definition",{"type":188,"tag":197,"props":263,"children":264},{},[265],{"type":194,"value":266},"Exact statement or description of the nature, scope, or meaning of a word, concept, or object.",{"type":188,"tag":189,"props":268,"children":270},{"id":269},"diagnosis",[271],{"type":194,"value":272},"Diagnosis",{"type":188,"tag":197,"props":274,"children":275},{},[276],{"type":194,"value":277},"By way of knowledge. (Greek: Dia - Gnosis)",{"type":188,"tag":189,"props":279,"children":281},{"id":280},"force",[282],{"type":194,"value":283},"Force",{"type":188,"tag":197,"props":285,"children":286},{},[287],{"type":194,"value":288},"Action that causes physical change which is in harmony with Natural Law; because it doesn’t cause harm to other sentient beings.",{"type":188,"tag":189,"props":290,"children":292},{"id":291},"free",[293],{"type":194,"value":294},"Free",{"type":188,"tag":197,"props":296,"children":297},{},[298],{"type":194,"value":299},"Not under the control or in the power of another; able to act or be done as one wishes.",{"type":188,"tag":189,"props":301,"children":303},{"id":302},"great-work",[304],{"type":194,"value":305},"Great Work",{"type":188,"tag":197,"props":307,"children":308},{},[309],{"type":194,"value":310},"The work of teaching Natural Law to others; after one has come to a definite understanding of its workings.",{"type":188,"tag":189,"props":312,"children":314},{"id":313},"ignorance",[315],{"type":194,"value":316},"Ignorance",{"type":188,"tag":197,"props":318,"children":319},{},[320],{"type":194,"value":321},"To not know something even though necessary information is present; because that information is wilfully being ignored. (Latin: Ignorare)",{"type":188,"tag":189,"props":323,"children":325},{"id":324},"initiation",[326],{"type":194,"value":327},"Initiation",{"type":188,"tag":197,"props":329,"children":330},{},[331],{"type":194,"value":332},"To Begin. To Introduce. To Start.",{"type":188,"tag":189,"props":334,"children":336},{"id":335},"jurisdiction",[337],{"type":194,"value":338},"Jurisdiction",{"type":188,"tag":197,"props":340,"children":341},{},[342],{"type":194,"value":343},"To say what the law is; to “decide” what right and wrong are. (Latin: Jus - Dictere)",{"type":188,"tag":189,"props":345,"children":347},{"id":346},"justification",[348],{"type":194,"value":349},"Justification",{"type":188,"tag":197,"props":351,"children":352},{},[353],{"type":194,"value":354},"To “make” or “create” a right. (Latin: Jus - Facere)",{"type":188,"tag":189,"props":356,"children":358},{"id":357},"knowledge",[359],{"type":194,"value":5},{"type":188,"tag":197,"props":361,"children":362},{},[363],{"type":194,"value":364},"Available information.",{"type":188,"tag":189,"props":366,"children":368},{"id":367},"law",[369],{"type":194,"value":370},"Law",{"type":188,"tag":197,"props":372,"children":373},{},[374],{"type":194,"value":375},"An existing condition which is both binding and immutable.",{"type":188,"tag":189,"props":377,"children":379},{"id":378},"maven",[380],{"type":194,"value":381},"Maven",{"type":188,"tag":197,"props":383,"children":384},{},[385,387,395,397,405,407,417],{"type":194,"value":386},"\"One who understands\". \"Truely Awake\". The word Maven derives from the Yiddish ",{"type":188,"tag":218,"props":388,"children":389},{},[390],{"type":188,"tag":214,"props":391,"children":392},{},[393],{"type":194,"value":394},"Meyvn",{"type":194,"value":396}," and Hebrew word ",{"type":188,"tag":218,"props":398,"children":399},{},[400],{"type":188,"tag":214,"props":401,"children":402},{},[403],{"type":194,"value":404},"Mevyn",{"type":194,"value":406},". To be a Maven you have to be more than just understand a topic, you have to ",{"type":188,"tag":408,"props":409,"children":410},"b",{},[411],{"type":188,"tag":412,"props":413,"children":414},"u",{},[415],{"type":194,"value":416},"know",{"type":194,"value":418}," its ins and outs, and act on the knowledge when it is required (wisdom). You don't become a Maven overnight, that kind of expertise and understanding comes with an accumulation of knowledge over the years.",{"type":188,"tag":189,"props":420,"children":422},{"id":421},"natural",[423],{"type":194,"value":424},"Natural",{"type":188,"tag":197,"props":426,"children":427},{},[428],{"type":194,"value":429},"Having come from Spirit / God. (Egyptian: Neter)",{"type":188,"tag":189,"props":431,"children":433},{"id":432},"natural-law",[434],{"type":194,"value":24},{"type":188,"tag":197,"props":436,"children":437},{},[438],{"type":194,"value":439},"Natural Law is a set of universal, objective, eternal, non-man-made, binding and immutable conditions which govern the consequences of behaviours of beings with the capacity for understanding the difference between harmful and non-harmful actions.",{"type":188,"tag":189,"props":441,"children":443},{"id":442},"nescience",[444],{"type":194,"value":445},"Nescience",{"type":188,"tag":197,"props":447,"children":448},{},[449],{"type":194,"value":450},"To not know something because necessary information was absent or unattainable. (Latin: Nescire)",{"type":188,"tag":189,"props":452,"children":454},{"id":453},"occult",[455],{"type":194,"value":456},"Occult",{"type":188,"tag":197,"props":458,"children":459},{},[460],{"type":194,"value":461},"Hidden from sight. (Latin: Occultus / Occultare)",{"type":188,"tag":189,"props":463,"children":465},{"id":464},"order",[466],{"type":194,"value":467},"Order",{"type":188,"tag":197,"props":469,"children":470},{},[471],{"type":194,"value":472},"The conditions which we truly want to manifest into physical reality.",{"type":188,"tag":189,"props":474,"children":476},{"id":475},"order-follower",[477],{"type":194,"value":478},"Order-Follower",{"type":188,"tag":197,"props":480,"children":481},{},[482],{"type":194,"value":483},"A person that causes the currently existing system of slavery to remain in place.",{"type":188,"tag":189,"props":485,"children":487},{"id":486},"petition",[488],{"type":194,"value":489},"Petition",{"type":188,"tag":197,"props":491,"children":492},{},[493],{"type":194,"value":494},"A formally drawn request, that is addressed to a person or group of persons in authority or power, soliciting some favour, right, mercy, or other benefit.",{"type":188,"tag":189,"props":496,"children":498},{"id":497},"principle",[499],{"type":194,"value":500},"Principle",{"type":188,"tag":197,"props":502,"children":503},{},[504],{"type":194,"value":505},"That which comes first; that which is most important. (Latin: Principia)",{"type":188,"tag":189,"props":507,"children":509},{"id":508},"religion",[510],{"type":194,"value":511},"Religion",{"type":188,"tag":197,"props":513,"children":514},{},[515],{"type":194,"value":516},"To hold back from forward progress. (Latin: Religare)",{"type":188,"tag":189,"props":518,"children":520},{"id":519},"responsibility",[521],{"type":194,"value":522},"Responsibility",{"type":188,"tag":197,"props":524,"children":525},{},[526],{"type":194,"value":527},"The ability to respond.",{"type":188,"tag":189,"props":529,"children":531},{"id":530},"right",[532],{"type":194,"value":533},"Right",{"type":188,"tag":197,"props":535,"children":536},{},[537],{"type":194,"value":538},"Correct and Moral. Any action which does not cause harm to another sentient being or their property.",{"type":188,"tag":189,"props":540,"children":542},{"id":541},"self-loathing",[543],{"type":194,"value":544},"Self-Loathing",{"type":188,"tag":197,"props":546,"children":547},{},[548],{"type":194,"value":549},"The underlying psychological condition that causes people to attempt to abdicate their own personal responsibility to exercise conscience, and fall into patterns of order-following and justification.",{"type":188,"tag":189,"props":551,"children":553},{"id":552},"solipsist",[554],{"type":194,"value":555},"Solipsist",{"type":188,"tag":197,"props":557,"children":558},{},[559],{"type":194,"value":560},"A person that believes they are the only being in existence (Latin: Solus - Ipse)",{"type":188,"tag":189,"props":562,"children":564},{"id":563},"statist",[565],{"type":194,"value":566},"Statist",{"type":188,"tag":197,"props":568,"children":569},{},[570],{"type":194,"value":571},"A person who erroneously believes in “authority.”",{"type":188,"tag":189,"props":573,"children":575},{"id":574},"truth",[576],{"type":194,"value":577},"Truth",{"type":188,"tag":197,"props":579,"children":580},{},[581],{"type":194,"value":582},"All that has happened in the past, and that which is happening in the present moment; that which has been, and that which is.",{"type":188,"tag":189,"props":584,"children":586},{"id":585},"violence",[587],{"type":194,"value":588},"Violence",{"type":188,"tag":197,"props":590,"children":591},{},[592],{"type":194,"value":593},"Action that causes physical change which is in not harmony with Natural Law; because it causes harm to other sentient beings.",{"type":188,"tag":189,"props":595,"children":597},{"id":596},"wisdom",[598],{"type":194,"value":599},"Wisdom",{"type":188,"tag":197,"props":601,"children":602},{},[603],{"type":194,"value":604},"Either action, or inaction; based on knowledge and understanding.",{"type":188,"tag":189,"props":606,"children":608},{"id":607},"wrong",[609],{"type":194,"value":610},"Wrong",{"type":188,"tag":197,"props":612,"children":613},{},[614],{"type":194,"value":615},"Incorrect / immoral; actions that cause harm to other sentient beings.",{"title":182,"searchDepth":617,"depth":617,"links":618},2,[619,620,621,622,623,624,625,626,627,628,629,630,631,632,633,634,635,636,637,638,639,640,641,642,643,644,645,646,647,648,649,650,651,652],{"id":191,"depth":617,"text":195},{"id":204,"depth":617,"text":207},{"id":236,"depth":617,"text":239},{"id":247,"depth":617,"text":250},{"id":258,"depth":617,"text":261},{"id":269,"depth":617,"text":272},{"id":280,"depth":617,"text":283},{"id":291,"depth":617,"text":294},{"id":302,"depth":617,"text":305},{"id":313,"depth":617,"text":316},{"id":324,"depth":617,"text":327},{"id":335,"depth":617,"text":338},{"id":346,"depth":617,"text":349},{"id":357,"depth":617,"text":5},{"id":367,"depth":617,"text":370},{"id":378,"depth":617,"text":381},{"id":421,"depth":617,"text":424},{"id":432,"depth":617,"text":24},{"id":442,"depth":617,"text":445},{"id":453,"depth":617,"text":456},{"id":464,"depth":617,"text":467},{"id":475,"depth":617,"text":478},{"id":486,"depth":617,"text":489},{"id":497,"depth":617,"text":500},{"id":508,"depth":617,"text":511},{"id":519,"depth":617,"text":522},{"id":530,"depth":617,"text":533},{"id":541,"depth":617,"text":544},{"id":552,"depth":617,"text":555},{"id":563,"depth":617,"text":566},{"id":574,"depth":617,"text":577},{"id":585,"depth":617,"text":588},{"id":596,"depth":617,"text":599},{"id":607,"depth":617,"text":610},"markdown","content:4.resources:2.definitions.md","content","4.resources/2.definitions.md","md",1697964474468]